<idea-plugin>

<id>swingexplorer-plugin-v3</id>
  <name>SwingExplorer Integration v3</name>
  <vendor email="dominik@haberlah.eu">Dominik Haberlah</vendor>

  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.java</depends>

  <extensions defaultExtensionNs="com.intellij">
    <executor implementation="com.github.dhaberlah.swingexplorer.Executor"/>
    <programRunner implementation="com.github.dhaberlah.swingexplorer.Runner"/>
    <executor implementation="com.github.dhaberlah.swingexplorer.DebugExecutor"/>
    <programRunner implementation="com.github.dhaberlah.swingexplorer.DebugRunner"/>
  </extensions>

</idea-plugin>